background no
double_buffer yes
own_window no
out_to_console yes
text_buffer_size 2048
update_interval 1.0
out_to_x no

TEXT
^fg(red)Temp: ${hwmon temp 1}C \
^fg(green)${cpu cpu1}% \
^fg(green)${cpu cpu2}% \
^fg(green)${cpu cpu3}% \
^fg(green)${cpu cpu4}% \
^fg(ffffff)Mem:$memperc% \
^fg(ffffff)${downspeedf eth0}KiB \
/ ^fg(ffffff)${upspeedf eth0}KiB \
^fg(orange)${time %b %e %H:%M:%S}
